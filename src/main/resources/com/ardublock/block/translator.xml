            <command name="void LedBlinks(int LedPin, int Count, int TimeON, int TimeOFF)">
            void LedBlinks(int LedPin, int Count, int TimeON, int TimeOFF)
            {
            for(int i = 0; i &lt; Count; i++)
            {
            LedControl(LedPin, 1);
